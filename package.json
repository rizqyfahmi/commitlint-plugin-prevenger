{
  "name": "commitlint-plugin-prevenger",
  "version": "1.0.5",
  "main": "dist/index.js",
  "scripts": {
    "lint": "npx eslint src/ --ext .ts",
    "commit": "git-cz",
    "build": "tsc",
    "test": "jest --coverage",
    "clean": "rm -rf node_modules && rm package-lock.json && npm cache clean --force && npm install",
    "prepare": "husky"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rizqyfahmi/commitlint-plugin-prevenger.git"
  },
  "keywords": [
    "commitlint",
    "commitlintplugin",
    "commitlint-plugin",
    "scope",
    "pattern",
    "scope-pattern"
  ],
  "author": "Ahmad Yulia Rizqy Fahmi <https://github.com/rizqyfahmi>",
  "license": "MIT",
  "publishConfig": {
    "provenance": true
  },
  "bugs": {
    "url": "https://github.com/rizqyfahmi/commitlint-plugin-prevenger/issues"
  },
  "homepage": "https://github.com/rizqyfahmi/commitlint-plugin-prevenger#readme",
  "description": "A Commitlint plugin that is containing some toolkits for checking the commit message",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@stylistic/eslint-plugin": "^5.2.0",
    "@types/conventional-commits-parser": "^5.0.1",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.12.0",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "semantic-release": "^24.2.7",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  }
}
